{
	"name": "qvweb",
	"description":"image viewer for qv-10" ,
	"short_name": "qvweb",
	"start_url" : "./index.html",
	"id": "qvweb",
	"scope": "./",
	"display": "standalone",
	"theme_color": "#ffffff",
	"background_color": "#ffffff",
	"orientation": "any",
	"icons": [
		{
			"src": "./resources/icons/icon.png",
			"type": "image/png",
			"sizes": "700x700"
		}
	],
	"launch_handler": {
		"client_mode": "auto"
	},
	"screenshots": [
		{
			"src": "./resources/screenshots/screenshot.jpg",
			"sizes": "1080x760",
			"type": "image/jpeg",
			"form_factor": "wide",
			"label": "dummy"
		}
	]
}
